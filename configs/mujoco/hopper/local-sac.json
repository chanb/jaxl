{
    "logging_config": {
        "save_path": "./logs/mujoco-hopper",
        "experiment_name": "sac-unbounded_std",
        "log_interval": 1,
        "checkpoint_interval": false
    },
    "model_config": {
        "policy": {
            "architecture": "mlp",
            "layers": [
                256,
                256
            ],
            "activation": "tanh",
            "flatten": true
        },
        "qf": {
            "architecture": "ensemble",
            "model": {
                "architecture": "mlp",
                "layers": [256, 256],
                "flatten": true
            },
            "num_models": 2,
            "vmap_all": false
        },
        "qf_encoding": {
            "q_function": "state_action_input",
            "type": "concatenate_inputs_encoding",
            "kwargs": {}
        }
    },
    "optimizer_config": {
        "policy": {
            "optimizer": "adam",
            "lr": {
                "scheduler": "constant_schedule",
                "scheduler_kwargs": {
                    "value": 0.0003
                }
            },
            "max_grad_norm": false
        },
        "qf": {
            "optimizer": "adam",
            "lr": {
                "scheduler": "constant_schedule",
                "scheduler_kwargs": {
                    "value": 0.0003
                }
            },
            "max_grad_norm": false
        },
        "temp": {
            "optimizer": "adam",
            "lr": {
                "scheduler": "constant_schedule",
                "scheduler_kwargs": {
                    "value": 0.0003
                }
            },
            "max_grad_norm": false
        }
    },
    "learner_config": {
        "task": "reinforcement_learning",
        "env_config": {
            "env_type": "gym",
            "env_name": "Hopper-v3",
            "env_kwargs": {}
        },
        "seeds": {
            "model_seed": 42,
            "buffer_seed": 42,
            "env_seed": 42,
            "learner_seed": 42
        },
        "buffer_config": {
            "buffer_type": "default",
            "buffer_size": 1000000
        },
        "buffer_warmup": 1000,
        "exploration_policy": {
            "policy_distribution": "squashed_gaussian",
            "kwargs": {
                "means": 0.0,
                "stds": 0.5
            }
        },
        "num_steps_per_epoch": 5000,
        "batch_size": 256,
        "learner": "sac",
        "obs_rms": false,
        "value_rms": false,
        "policy_distribution": "squashed_gaussian",
        "std_transform": "squareplus",
        "gamma": 0.99,
        "update_frequency": 1,
        "target_entropy": "auto",
        "initial_temperature": 0.2,
        "qf_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean"
        },
        "pi_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean"
        },
        "temp_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean"
        },
        "actor_update_frequency": 1,
        "target_update_frequency": 1,
        "tau": 5e-3
    },
    "train_config": {
        "num_epochs": 200
    }
}