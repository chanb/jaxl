{
    "logging_config": {
        "save_path": "./logs/classic_control/cartpole",
        "experiment_name": "reinforce",
        "log_interval": 10,
        "checkpoint_interval": 100
    },
    "model_config": {
        "architecture": "mlp",
        "layers": [
            64,
            64
        ],
        "activation": "tanh",
        "flatten": true
    },
    "optimizer_config": {
        "optimizer": "adam",
        "lr": {
            "scheduler": "constant_schedule",
            "scheduler_kwargs": {
                "value": 3e-4
            }
        },
        "max_grad_norm": false
    },
    "learner_config": {
        "task": "reinforcement_learning",
        "env_config": {
            "env_type": "gym",
            "env_name": "CartPole-v1",
            "env_kwargs": {}
        },
        "seeds": {
            "model_seed": 42,
            "buffer_seed": 42,
            "env_seed": 42
        },
        "buffer_config": {
            "buffer_type": "default",
            "buffer_size": 256
        },
        "num_steps_per_epoch": 2048,
        "learner": "reinforce",
        "gamma": 0.99,
        "eps": 1e-05,
        "obs_rms": true,
        "value_rms": false,
        "policy_distribution": "softmax",
        "min_std": 1e-06,
        "pi_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean"
        }
    },
    "train_config": {
        "num_epochs": 1000
    }
}
