{
    "logging_config": {
        "save_path": "./logs/icl-mnist",
        "experiment_name": "context_len_16-num_blocks_4-fixed_length-random_label-stratified",
        "log_interval": 1,
        "checkpoint_interval": 1
    },
    "model_config": {
        "architecture": "icl_gpt",
        "num_contexts": 16,
        "num_blocks": 4,
        "num_heads": 1,
        "embed_dim": 32,
        "positional_encoding": {
            "type": "default",
            "kwargs": {
                "embed_dim": 32,
                "max_len": 17
            }
        },
        "input_tokenizer": {
            "type": "cnn",
            "kwargs": {
                "features": [32, 32, 32],
                "kernel_sizes": [[3, 3], [3, 3], [3, 3]],
                "layers": [32]
            }
        },
        "output_tokenizer": {
            "type": "mlp",
            "kwargs": {
                "layers": [10]
            }
        },
        "query_pred_only": true
    },
    "optimizer_config": {
        "optimizer": "adam",
        "lr": {
            "scheduler": "constant_schedule",
            "scheduler_kwargs": {
                "value": 0.0003
            }
        },
        "max_grad_norm": false
    },
    "learner_config": {
        "task": "in_context_learning",
        "dataset_config": {
            "dataset_name": "mnist",
            "dataset_kwargs": {
                "save_path": "../",
                "task_name": "stratified_multitask_mnist_finegrain",
                "task_config": {
                    "num_sequences": 500000,
                    "num_queries": 30,
                    "random_label": true,
                    "save_dir": "/home/bryanpu1/projects/icl/data/mnist_icl"
                }
            },
            "dataset_wrapper": {
                "type": "RepeatedContextDataset",
                "kwargs": {
                    "context_len": 16
                }
            },
            "num_workers": 4
        },
        "seeds": {
            "model_seed": 42,
            "data_seed": 42
        },
        "learner": "mle",
        "losses": ["categorical", "l2"],
        "loss_settings": [
            {
                "coefficient": 1.0,
                "reduction": "mean",
                "is_one_hot": true
            },
            {
                "coefficient": 0.0
            }
        ],
        "num_updates_per_epoch": 1,
        "batch_size": 32,
        "predictor_type": "default"
    },
    "train_config": {
        "num_epochs": 10
    }
}
