{
    "logging_config": {
        "save_path": "./logs/car_racing",
        "experiment_name": "ppo",
        "log_interval": 1,
        "checkpoint_interval": 5
    },
    "model_config": {
        "policy": {
            "architecture": "mlp",
            "layers": [128, 128]
        },
        "vf": {
            "architecture": "mlp",
            "layers": [128, 128]
        }
    },
    "optimizer_config": {
        "policy": {
            "optimizer": "adam",
            "lr": {
                "scheduler": "constant_schedule",
                "scheduler_kwargs": {
                    "value": 1e-3
                }
            },
            "max_grad_norm": 0.5,
            "weight_decay": 0.1
        },
        "vf": {
            "optimizer": "adam",
            "lr": {
                "scheduler": "constant_schedule",
                "scheduler_kwargs": {
                    "value": 1e-3
                }
            },
            "max_grad_norm": 0.5
        }
    },
    "learner_config": {
        "task": "reinforcement_learning",
        "env_config": {
            "env_type": "gym",
            "env_name": "CarRacing-v2",
            "env_kwargs": {
                "seed": 0,
                "use_default": false
            }
        },
        "seeds": {
            "model_seed": 42,
            "buffer_seed": 42,
            "env_seed": 42
        },
        "buffer_config": {
            "buffer_type": "default",
            "buffer_size": 2048
        },
        "num_steps_per_epoch": 2048,
        "learner": "ppo",
        "gamma": 0.99,
        "gae_lambda": 0.95,
        "normalize_advantage": true,
        "temperature": 1.0,
        "eps": 1e-5,
        "obs_rms": true,
        "value_rms": false,
        "policy_distribution": "softmax",
        "opt_batch_size": 128,
        "opt_epochs": 80,
        "kl_threshold": 0.015,
        "update_before_early_stopping": true,
        "ent_loss_setting": {
            "scheduler": "constant_schedule",
            "scheduler_kwargs": {
                "value": 0.0
            }
        },
        "vf_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean",
            "clip_param": 0.2
        },
        "pi_loss_setting": {
            "objective": "clip",
            "coefficient": 1.0,
            "reduction": "mean",
            "clip_param": 0.2,
            "beta": 0.0
        }
    },
    "train_config": {
        "num_epochs": 6
    }
}
