        "learner": "mle",
        "losses": ["categorical", "l2"],
        "loss_settings": [
            {
                "coefficient": 1.0,
                "reduction": "mean",
                "is_one_hot": true
            },
            {
                "coefficient": 0.0
            }
        ],
        "num_updates_per_epoch": 1,
        "batch_size": 32,
        "predictor_type": "default"
