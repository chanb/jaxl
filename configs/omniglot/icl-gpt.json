{
    "logging_config": {
        "save_path": "./logs/icl-omniglot",
        "experiment_name": "cnn-context_len_16-num_blocks_8",
        "log_interval": 50,
        "checkpoint_interval": 10000
    },
    "model_config": {
        "architecture": "icl_gpt",
        "num_contexts": 16,
        "num_blocks": 8,
        "num_heads": 8,
        "embed_dim": 64,
        "positional_encoding": {
            "type": "default",
            "kwargs": {
                "embed_dim": 64,
                "max_len": 17
