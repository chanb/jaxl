    "learner_config": {
        "task": "in_context_learning",
        "dataset_config": {
            "dataset_name": "omniglot",
            "dataset_kwargs": {
                "save_path": "/home/bryanpu1/projects/icl/data",
                "task_name": "multitask_omniglot_finegrain",
                "task_config": {
                    "num_sequences": 100000,
