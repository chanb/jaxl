{
    "logging_config": {
        "save_path": "./logs/icl-sigmoid_bce-context_len_8-large_arch",
        "experiment_name": "all_ones",
        "log_interval": 10,
        "checkpoint_interval": 1000
    },
    "model_config": {
        "architecture": "icl_gpt",
        "num_contexts": 8,
        "num_blocks": 4,
        "num_heads": 1,
        "embed_dim": 32,
        "positional_encoding": {
            "type": "default",
            "kwargs": {
                "embed_dim": 32,
                "max_len": 9
            }
        },
        "query_pred_only": true
    },
    "optimizer_config": {
        "optimizer": "adam",
        "lr": {
            "scheduler": "constant_schedule",
            "scheduler_kwargs": {
                "value": 0.0003
            }
        },
        "max_grad_norm": false,
        "mask_names": ["predictor"]
    },
    "learner_config": {
        "task": "in_context_learning",
        "dataset_config": {
            "dataset_name": "multitask_nd_linear_classification",
            "dataset_kwargs": {
                "input_dim": 2,
                "num_sequences": 100000,
                "sequence_length": 41,
                "noise": 0.0,
                "params_bound": [-10.0, 10.0],
                "num_active_params": null,
                "val_frac": 0.0005,
                "margin": 0.2
            },
            "dataset_wrapper": {
                "type": "ContextDataset",
                "kwargs": {
                    "context_len": 8
                }
            }
        },
        "seeds": {
            "model_seed": 42,
            "data_seed": 42
        },
        "learner": "mle",
        "losses": ["sigmoid_bce", "l2"],
        "loss_settings": [
            {
                "coefficient": 1.0,
                "reduction": "mean",
                "is_one_hot": true
            },
            {
                "coefficient": 0.0
            }
        ],
        "num_updates_per_epoch": 100,
        "batch_size": 100,
        "predictor_type": "all_ones"
    },
    "train_config": {
        "num_epochs": 5000
    }
}