{
    "logging_config": {
        "save_path": "./logs/icl-sigmoid_bce-context_len_20-num_blocks_8",
        "experiment_name": "one_hot",
        "log_interval": 10,
        "checkpoint_interval": 1000
    },
    "model_config": {
        "architecture": "icl_gpt",
        "num_contexts": 20,
        "num_blocks": 8,
        "num_heads": 1,
        "embed_dim": 64,
        "positional_encoding": {
            "type": "default",
            "kwargs": {
                "embed_dim": 64,
                "max_len": 21
            }
        },
        "query_pred_only": true
    },
    "optimizer_config": {
        "optimizer": "adam",
        "lr": {
            "scheduler": "constant_schedule",
            "scheduler_kwargs": {
                "value": 0.0003
            }
        },
        "max_grad_norm": false,
        "mask_names": ["predictor"]
    },
    "learner_config": {
        "task": "in_context_learning",
        "dataset_config": {
            "dataset_name": "multitask_nd_linear_classification",
            "dataset_kwargs": {
                "input_dim": 2,
                "num_sequences": 500000,
                "sequence_length": 30,
                "noise": 0.0,
                "params_bound": [-10.0, 10.0],
                "num_active_params": null,
                "margin": 0.2,
                "save_path": "./data/icl-sigmoid_bce-context_len_20-num_blocks_8-500k_tasks.pkl"
            },
            "dataset_wrapper": {
                "type": "ContextDataset",
                "kwargs": {
                    "context_len": 20
                }
            }
        },
        "seeds": {
            "model_seed": 42,
            "data_seed": 42
        },
        "learner": "mle",
        "losses": ["sigmoid_bce"],
        "loss_settings": [
            {
                "coefficient": 1.0,
                "reduction": "mean",
                "is_one_hot": true
            }
        ],
        "num_updates_per_epoch": 100,
        "batch_size": 256,
        "learner_settings": {
            "store_val_preds": false
        },
        "predictor_type": "one_hot"
    },
    "train_config": {
        "num_epochs": 40000
    }
}