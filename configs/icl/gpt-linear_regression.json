{
    "logging_config": {
        "save_path": "./logs/icl-noiseless-no_bias-20d_linear-active_dim_4-full_context_16",
        "experiment_name": "gpt-pos_enc",
        "log_interval": 10,
        "checkpoint_interval": 10000
    },
    "model_config": {
        "architecture": "icl_gpt",
        "num_contexts": 16,
        "num_blocks": 2,
        "num_heads": 1,
        "embed_dim": 32,
        "out_dim": 1,
        "positional_encoding": {
            "type": "default",
            "kwargs": {
                "embed_dim": 32,
                "max_len": 17
            }
        }
    },
    "optimizer_config": {
        "optimizer": "adam",
        "lr": {
            "scheduler": "constant_schedule",
            "scheduler_kwargs": {
                "value": 0.0003
            }
        },
        "max_grad_norm": false
    },
    "learner_config": {
        "task": "in_context_learning",
        "dataset_config": {
            "dataset_name": "multitask_nd_linear_regression",
            "dataset_kwargs": {
                "input_dim": 20,
                "num_sequences": 100000,
                "sequence_length": 41,
                "noise": 0.0,
                "params_bound": [-10.0, 10.0],
                "num_active_params": 4,
                "val_frac": 0.0005
            },
            "dataset_wrapper": {
                "type": "ContextDataset",
                "kwargs": {
                    "context_len": 16
                }
            }
        },
        "seeds": {
            "model_seed": 42,
            "data_seed": 42
        },
        "learner": "mle",
        "losses": ["gaussian", "l2"],
        "loss_settings": [
            {
                "coefficient": 1.0,
                "reduction": "mean"
            },
            {
                "coefficient": 0.0
            }
        ],
        "num_updates_per_epoch": 100,
        "batch_size": 100
    },
    "train_config": {
        "num_epochs": 2000
    }
}