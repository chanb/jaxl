{
    "logging_config": {
        "save_path": "./logs/mountaincarcontinuous",
        "experiment_name": "ppo",
        "log_interval": 1,
        "checkpoint_interval": 10
    },
    "model_config": {
        "policy": {
            "architecture": "mlp",
            "layers": [128, 128]
        },
        "vf": {
            "architecture": "mlp",
            "layers": [128, 128]
        }
    },
    "optimizer_config": {
        "policy": {
            "optimizer": "adam",
            "lr": 3e-4,
            "max_grad_norm": false
        },
        "vf": {
            "optimizer": "adam",
            "lr": 3e-4,
            "max_grad_norm": false
        }
    },
    "learner_config": {
        "task": "reinforcement_learning",
        "env_config": {
            "env_type": "gym",
            "env_name": "MountainCarContinuous-v0",
            "env_kwargs": {}
        },
        "seeds": {
            "model_seed": 42,
            "buffer_seed": 42,
            "env_seed": 42
        },
        "buffer_config": {
            "buffer_type": "default",
            "buffer_size": 2048
        },
        "learner": "ppo",
        "num_steps_per_epoch": 2048,
        "gamma": 0.99,
        "gae_lambda": 0.95,
        "normalize_advantage": true,
        "eps": 1e-5,
        "obs_rms": true,
        "value_rms": true,
        "policy_distribution": "gaussian",
        "min_std": 1e-7,
        "opt_batch_size": 1024,
        "opt_epochs": 10,
        "vf_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean",
            "clip_param": 0.1
        },
        "pi_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean",
            "clip_param": 0.1
        }
    },
    "train_config": {
        "num_epochs": 1000
    }
}
