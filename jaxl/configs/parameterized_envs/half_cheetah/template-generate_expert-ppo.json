{
    "logging_config": {
        "save_path": "./logs/half_cheetah",
        "experiment_name": "ppo",
        "log_interval": 1,
        "checkpoint_interval": 10
    },
    "model_config": {
        "policy": {
            "architecture": "mlp",
            "layers": [128, 128]
        },
        "vf": {
            "architecture": "mlp",
            "layers": [128, 128]
        }
    },
    "optimizer_config": {
        "policy": {
            "optimizer": "adam",
            "lr": 3e-4,
            "max_grad_norm": 1.0
        },
        "vf": {
            "optimizer": "adam",
            "lr": 3e-4,
            "max_grad_norm": 1.0
        }
    },
    "learner_config": {
        "task": "reinforcement_learning",
        "env_config": {
            "env_type": "gym",
            "env_name": "ParameterizedHalfCheetah-v0",
            "env_kwargs": {
                "seed": 42
            }
        },
        "seeds": {
            "model_seed": 42,
            "buffer_seed": 42,
            "env_seed": 42
        },
        "buffer_config": {
            "buffer_type": "default",
            "buffer_size": 128
        },
        "num_steps_per_epoch": 2048,
        "learner": "ppo",
        "gamma": 0.99,
        "gae_lambda": 0.9,
        "normalize_advantage": true,
        "eps": 1e-5,
        "obs_rms": true,
        "value_rms": true,
        "policy_distribution": "gaussian",
        "min_std": 1e-4,
        "opt_batch_size": 64,
        "opt_epochs": 2,
        "vf_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean",
            "clip_param": 0.2
        },
        "pi_loss_setting": {
            "coefficient": 1.0,
            "reduction": "mean",
            "clip_param": 0.2
        }
    },
    "train_config": {
        "num_epochs": 1000
    }
}
