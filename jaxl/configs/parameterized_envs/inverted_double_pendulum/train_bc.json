{
    "logging_config": {
        "save_path": "./logs/inverted_double_pendulum",
        "experiment_name": "bc",
        "log_interval": 1,
        "checkpoint_interval": 500
    },
    "model_config": {
        "architecture": "mlp",
        "layers": [128, 128]
    },
    "optimizer_config": {
        "optimizer": "adam",
        "lr": 3e-4,
        "max_grad_norm": 10.0
    },
    "learner_config": {
        "task": "imitation_learning",
        "buffer_config": {
            "load_buffer": "<TO_MODIFY>",
            "buffer_type": "default"
        },
        "seeds": {
            "model_seed": 42,
            "buffer_seed": 42
        },
        "learner": "bc",
        "num_updates_per_epoch": 100,
        "batch_size": 128,
        "obs_rms": false,
        "losses": ["gaussian", "l2"],
        "loss_settings": [
            {
                "coefficient": 1.0,
                "reduction": "mean"
            },
            {
                "coefficient": 0.0002
            }
        ]
    },
    "train_config": {
        "num_epochs": 10000
    }
